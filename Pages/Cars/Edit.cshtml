@page
@model Car_Wash.Pages.Cars.EditModel

@{
    ViewData["Title"] = "Edit";
}

<h1 class="pt-4"> </h1>
<h4>Car</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Car.id" />
            <div class="form-group">
                <label asp-for="Car.Registration_no" class="control-label"></label>
                <input asp-for="Car.Registration_no" class="form-control" />
                <span asp-validation-for="Car.Registration_no" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Car.Make" class="control-label"></label>
                <input asp-for="Car.Make" class="form-control" />
                <span asp-validation-for="Car.Make" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-page="./Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
